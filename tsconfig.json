{
    "compilerOptions": {
      "target": "ES6", 
      "module": "ES6",
      "moduleResolution": "Node",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./dist",
      "rootDir": "./src",
      "lib": ["ES2016", "DOM"],
    },
    "include": ["src"],
    "exclude": ["node_modules", "dist"]
}